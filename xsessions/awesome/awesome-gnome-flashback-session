#!/bin/sh

if [ -z $XDG_CURRENT_DESKTOP ]; then
    export XDG_CURRENT_DESKTOP="awesome-GNOME-Flashback:GNOME-Flashback:GNOME"
fi

# TODO move all this into lua scripts
xscreensaver -no-splash &

